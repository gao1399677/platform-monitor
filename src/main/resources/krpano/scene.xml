<scene name="[sceneName]" title="[sceneTitle]" onstart="js(getHotspot([sceneId]))" thumburl="/monitor/[projectId]/panos/[sceneId].tiles/thumb.jpg" lat=""
       lng="" heading="">

    <control bouncinglimits="calc:image.cube ? true : false"/>

    <view hlookat="0.0" vlookat="0.0" fovtype="MFOV" fov="120" maxpixelzoom="2.0" fovmin="70" fovmax="140"
          limitview="auto"/>

    <preview url="/monitor/[projectId]/panos/[sceneId].tiles/preview.jpg"/>

    <image>
        <cube url="/monitor/[projectId]/panos/[sceneId].tiles/%s/l%l/%v/l%l_%s_%v_%h.jpg"
              multires="[multiresSize],[multiresResolution]"/>
    </image>
</scene>